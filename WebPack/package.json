{
  "name": "curso-de-webpack",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack js/index.js -o js/bundle.js --mode development",
    "build:local": "webpack",
    "build:external": "webpack --config ./external/*.config.js",
    "build:css": "webpack --config ./css-style-loader/*.config.js",
    "build:extract:css": "webpack --config ./plugin-extract-text/*.config.js",
    "build:multiples:entry": "webpack --config ./multiples-entry-points/*.config.js",
    "_build:dev": "webpack --config ./webpack-dev-server/*.config.js --watch",
    "build:dev": "webpack-dev-server --config ./webpack-dev-server/webpack.config.js",
    "build:babel": "webpack --config ./babel-loader/webpack.config.js",
    "build:img": "webpack --config ./url-loader-img/webpack.config.js --mode development",
    "build:fonts": "webpack --config ./url-loader-fonts/webpack.config.js --mode development",
    "build:videos": "webpack --config ./url-loader-videos/*.config.js --mode development"
  },
  "author": "Victor Gamez <victor.gamez1596@gmail.com>",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-loader": "7.1.3",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-es2016": "6.24.1",
    "css-loader": "0.28.10",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "json-loader": "^0.5.7",
    "style-loader": "0.20.2",
    "url-loader": "1.0.1",
    "webpack": "4.0.1",
    "webpack-cli": "2.0.10",
    "webpack-dev-server": "3.1.0"
  }
}
